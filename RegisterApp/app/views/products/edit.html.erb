<%= button_to "Main Menu", main_menu_index_path, method: :get %>
<%= button_to "Product Listing", products_path, method: :get %>
<br>
<%= form_with model: @product, method: "put" do |f| %>
    <%= f.label :lookupCode, "LookupCode" %>
    <%= f.text_field :lookupCode, readonly: true %><br>
    <%= f.label :price, "Price" %>
    <%= f.text_field :price, :required => true %> <br>
    <%= f.label :count, "Count" %>
    <%= f.text_field :count, :required => true %><br>
    <%= f.submit "Edit" %>
<% end %>
<%= button_to "Delete", product_path, method: :delete %>