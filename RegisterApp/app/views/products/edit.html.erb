<%= form_with model: @product do |f| %>
    <%= f.label :id, "Look-UP Code" %>
    <%= f.text_field :id, :required => true%></br>
    <%= f.label :count, "Count" %>
    <%= f.text_field :count, :required => true%></br>
    <%= f.submit "Edit" %>
<% end %>
<%= link_to "Delete", product_path, method: delete %>